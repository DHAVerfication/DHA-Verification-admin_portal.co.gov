modules = ["web", "nodejs-20", "bash", "python-3.12"]
[agent]
expertMode = true
integrations = ["javascript_database:1.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Deployment Test"
author = 49482864
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "bash scripts/pre-deploy-test.sh"

[[workflows.workflow]]
name = "Validate Blueprint"
author = 49482864
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "bash scripts/validate-render-blueprint.sh"

[[workflows.workflow]]
name = "DHA Production Server"
author = 49770012
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "bash start-dev.sh"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Production Server"

[[workflows.workflow]]
name = "Production Server"
author = "agent"

[[workflows.workflow.tasks]]
